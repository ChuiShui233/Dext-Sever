ANON_ID_MODE=
# 严格模式下是否纳入端口（一般不建议）
ANON_INCLUDE_PORT=
# 在有反向代理(Nginx/Caddy)时建议开启，从 X-Forwarded-For 获取真实IP
TRUST_PROXY=
# 可选盐值
ANON_ID_SALT=
